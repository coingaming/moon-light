{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "dictionaryDefinitions": [
    {
      "name": "project-words",
      "path": "words.txt",
      "addWords": true
    }
  ],
  "enableGlobDot": true,
  "dictionaries": [
    "project-words"
  ],
  "ignorePaths": [
    "**/*.svg",
    "**/*.yml",
    "**/*.yaml",
    "**/package.json",
    ".git",
    "node_modules",
    "docs/generated/changelog.ts",
    "docs/CHANGELOG.md",
    ".config/aws-task-definition.json",
    ".config/Dockerfile",
    ".gitignore",
    ".vscode/extensions.json",
    "packages/base/src/private/presets/ds-moon-preset.js",
  ]
}
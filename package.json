  "license": "GPL-3.0-only",
  "dependencies": {
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "cpy-cli": "5.0.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5"
  }
}
